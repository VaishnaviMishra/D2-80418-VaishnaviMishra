import java.util.*;




public class Program {

	public static void main(String[] args) {
		System.out.println("go");
		int number = 20;
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		
		String binary  =  Integer.toBinaryString(num);
		System.out.println(binary);
        System.out.println("go");
	}

}
